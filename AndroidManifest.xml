<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.adam.lane.amortifier">
		<supports-screens android:resizeable="true"
		android:smallScreens="true" android:normalScreens="true"
		android:largeScreens="true" android:anyDensity="true"/>
	<uses-sdk android:minSdkVersion="6"></uses-sdk>
	<application android:icon="@drawable/icon">
		<activity android:name=".Amortifier" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".LoanEdit" />
		<activity android:name=".AmortizationActivity" />
	</application>

</manifest> 